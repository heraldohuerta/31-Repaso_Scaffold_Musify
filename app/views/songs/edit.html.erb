<h1> New Song <h1>

  <p id="notice"><%= notice %></p>



  <h1> Modificacion de una Cancion </h1>

  <%= form_with(model: [@playlist,@song], local: true) do |form| %>
  <div class="field">
    <%= form.label :artista %>
    <%= form.text_field :artis, id: :song_artist %>
  </div>

    <div class="field">
      <%= form.label :nombre %>
      <%= form.text_field :name, id: :song_name %>
    </div>

    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
